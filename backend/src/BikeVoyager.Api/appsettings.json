{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "ExternalPing": {
    "BaseUrl": "https://example.com"
  },
  "Geocoding": {
    "CommuneProvider": {
      "BaseUrl": "https://geo.api.gouv.fr",
      "TimeoutSeconds": 5
    },
    "AddressProvider": {
      "Enabled": true,
      "BaseUrl": "https://api-adresse.data.gouv.fr",
      "ApiKey": "",
      "TimeoutSeconds": 5
    },
    "Cache": {
      "SearchTtlSeconds": 300,
      "ReverseTtlSeconds": 600
    }
  },
  "Valhalla": {
    "BaseUrl": "http://localhost:8002",
    "TimeoutSeconds": 60,
    "Language": "fr-FR"
  },
  "Overpass": {
    "BaseUrl": "https://overpass-api.de/api/",
    "TimeoutSeconds": 25,
    "Cache": {
      "AroundRouteTtlSeconds": 300
    }
  },
  "CloudSync": {
    "BackupFolderName": "BikeVoyager",
    "GoogleDrive": {
      "ClientId": "",
      "ClientSecret": ""
    },
    "OneDrive": {
      "ClientId": "",
      "ClientSecret": ""
    }
  },
  "Feedback": {
    "Enabled": true,
    "RecipientEmail": "arnaud.wissart@live.fr",
    "SenderEmail": "",
    "SenderName": "BikeVoyager",
    "SubjectPrefix": "[BikeVoyager]",
    "Smtp": {
      "Host": "",
      "Port": 587,
      "UseSsl": true,
      "Username": "",
      "Password": ""
    }
  },
  "Cache": {
    "Redis": {
      "Configuration": "",
      "InstanceName": "bikevoyager:"
    }
  },
  "ApiSecurity": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "http://127.0.0.1:5173"
    ],
    "GeneralRequestsPerMinute": 180,
    "ComputeRequestsPerMinute": 40,
    "ExportRequestsPerMinute": 60,
    "EnforceOriginForUnsafeMethods": true,
    "AnonymousSessionCookieName": "bv_anon_sid",
    "AnonymousSessionLifetimeHours": 720
  },
  "AllowedHosts": "*"
}
